{
  "name": "static-website-template",
  "version": "0.0.1",
  "description": "Static website",
  "private": true,
  "engines": {
    "yarn": "^1.9",
    "node": "^8.9 || ^10.7"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "webpack-dev-server --host 0.0.0.0 --mode development --color --env.noclear",
    "build": "npm run build:staging",
    "build:staging": "webpack --display-error-details --mode development",
    "build:staging:minify": "npm run build:staging -- --env.fullminify",
    "build:prod": "webpack --display-error-details --mode production",
    "build:prod:minify": "npm run build:prod -- --env.fullminify",
    "postinstall": "npm run build:staging:minify"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "bodymovin": "^4.13.0",
    "compression": "^1.7.3",
    "detect-browser": "^3.0.0",
    "express": "^4.16.3",
    "gsap": "^2.0.1",
    "jquery": "^3.3.1",
    "reset-css": "^4.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.1.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "node-sass": "^4.9.2",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-preset-env": "^5.3.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "underscore-template-loader": "^1.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  }
}
