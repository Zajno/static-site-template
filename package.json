{
  "name": "static-website-template",
  "version": "0.0.1",
  "description": "Static website",
  "private": true,
  "engines": {
    "yarn": "^1.15",
    "node": "^8.9 || ^10.15"
  },
  "scripts": {
    "start": "node server/index.js",
    "dev": "webpack-dev-server --host 0.0.0.0 --mode development --color --env.noclear --env.hmr",
    "build": "webpack --display-error-details --mode development",
    "build:minify": "yarn build --env.fullminify",
    "build:prod": "webpack --display-error-details --mode production",
    "build:prod:minify": "yarn build:prod --env.fullminify",
    "build:prod:minify:env": "cross-env NODE_ENV=production yarn build:prod:minify",
    "heroku-postbuild": "yarn build:prod:minify && cd server && yarn"
  },
  "dependencies": {
    "@babel/polyfill": "^7.4.3",
    "core-js": "^3",
    "detect-browser": "^4.4.0",
    "gsap": "^2.1.2",
    "mobx": "^5.9.4",
    "regenerator-runtime": "^0.13.2",
    "reset-css": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "@types/gsap": "^1.20.2",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^2.0.1",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "raw-loader": "^1",
    "resolve-url-loader": "^3.1.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "underscore-template-loader": "^1.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
