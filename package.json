{
  "name": "@zajno/static-site-template",
  "version": "1.0.1",
  "description": "Static website template by Zajno",
  "private": true,
  "engines": {
    "yarn": "^1.22",
    "node": "^14"
  },
  "scripts": {
    "emulate": "firebase emulators:start --only hosting",
    "dev": "webpack serve --mode development --env noclear",
    "build": "webpack --mode development",
    "build:ts:w": "tsc --noEmit --watch",
    "build:minify": "yarn build --env fullminify",
    "build:prod": "webpack --node-env production --env fullminify",
    "build:release:staging": "yarn && (cross-env APP_ENV=staging yarn build:prod)",
    "build:release:production": "yarn && (cross-env APP_ENV=production yarn build:prod)",
    "deploy:firebase:staging": "firebase deploy --only hosting:web-stage",
    "deploy:firebase:production": "firebase deploy --only hosting:web-prod",
    "lint": "eslint \"app/**/*.ts\""
  },
  "dependencies": {
    "@zajno/common": "git+https://github.com/Zajno/common-utils.git#v1.0.4",
    "core-js": "^3.9.1",
    "detect-browser": "^5.2.0",
    "gsap": "file:./gsap-member/npm-install-this/gsap-bonus.tgz",
    "lottie-web": "^5.7.6",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@types/copy-webpack-plugin": "^6.4.0",
    "@types/core-js": "^2.5.4",
    "@types/css-minimizer-webpack-plugin": "^1.1.1",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^14.14.35",
    "@types/underscore": "^1.11.0",
    "@types/webpack": "^4.41.26",
    "@types/webpack-dev-server": "^3.11.2",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.0.0",
    "cross-env": "^7.0.3",
    "css-loader": "^5.1.3",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "^7.22.0",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^32.3.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.3.9",
    "node-sass": "^5.0.0",
    "postcss": "^8",
    "postcss-loader": "^5",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^11.0.1",
    "sitemap-webpack-plugin": "^1.1.0",
    "svgo-loader": "^3.0.0",
    "ts-loader": "^8.0.18",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3",
    "underscore": "^1.12.1",
