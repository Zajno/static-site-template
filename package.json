{
  "name": "static-website-template",
  "version": "0.0.1",
  "description": "Static website",
  "private": true,
  "engines": {
    "yarn": "^1.19",
    "node": "^10.16 || ^12.6"
  },
  "scripts": {
    "start": "node server/index.js",
    "dev": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack-dev-server --host 0.0.0.0 --mode development --color --env.noclear --env.hmr",
    "build": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --display-error-details --mode development",
    "build:minify": "yarn build --env.fullminify",
    "build:prod": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --display-error-details --mode production",
    "build:prod:minify": "yarn build:prod --env.fullminify",
    "build:prod:minify:env": "cross-env NODE_ENV=production yarn build:prod:minify",
    "heroku-postbuild": "yarn build:prod:minify && cd server && yarn"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "core-js": "^3",
    "detect-browser": "^4.8.0",
    "gsap": "^3.0.4",
    "lottie-web": "^5.5.10",
    "mobx": "^5.15.1",
    "regenerator-runtime": "^0.13.3",
    "reset-css": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@babel/plugin-syntax-dynamic-import": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@types/copy-webpack-plugin": "^5.0.0",
    "@types/core-js": "^2.5.2",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/mini-css-extract-plugin": "^0.9.0",
    "@types/node": "^13.1.1",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/ts-nameof": "^4.2.0",
    "@types/webpack": "^4.39.2",
    "@types/webpack-dev-server": "^3.1.7",
    "@typescript-eslint/eslint-plugin": "^2.13.0",
    "@typescript-eslint/parser": "^2.13.0",
    "autoprefixer": "^9.7.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^4",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.2",
    "ts-loader": "^6.2.1",
    "ts-nameof": "^4.2.2",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "ttypescript": "^1.5.8",
    "typescript": "^3.7.4",
    "underscore-template-loader": "^1.0.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  }
}
