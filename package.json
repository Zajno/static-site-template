{
  "name": "@zajno/static-site-template",
  "version": "1.0.1",
  "description": "Static website template by Zajno",
  "private": true,
  "engines": {
    "yarn": "^1.22",
    "node": "^14"
  },
  "scripts": {
    "emulate": "firebase emulators:start --only hosting",
    "dev": "webpack serve --mode development --env noclear",
    "build": "webpack --mode development",
    "build:ts:w": "tsc --noEmit --watch",
    "build:minify": "yarn build --env fullminify",
    "build:prod": "webpack --node-env production --env fullminify",
    "build:release:staging": "yarn && (cross-env APP_ENV=staging yarn build:prod)",
    "build:release:production": "yarn && (cross-env APP_ENV=production yarn build:prod)",
    "deploy:firebase:staging": "firebase deploy --only hosting:web-stage",
    "deploy:firebase:production": "firebase deploy --only hosting:web-prod",
    "lint": "eslint \"app/**/*.ts\""
  },
  "dependencies": {
    "@zajno/common": "^1.1.1",
    "core-js": "^3.12.0",
    "detect-browser": "^5.2.0",
    "gsap": "file:./gsap-member/npm-install-this/gsap-bonus.tgz",
    "lottie-web": "^5.7.8",
    "regenerator-runtime": "^0.13.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.16",
    "@babel/core": "^7.14.0",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.15",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.14.1",
    "@babel/preset-react": "^7.13.13",
    "@types/copy-webpack-plugin": "^8.0.0",
    "@types/core-js": "^2.5.4",
    "@types/css-minimizer-webpack-plugin": "^1.3.0",
    "@types/mini-css-extract-plugin": "^1.4.3",
    "@types/node": "^15.0.2",
    "@types/underscore": "^1.11.2",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "3.11.1",
    "@zajno/eslint-config": "^1.1.2",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.4",
    "css-minimizer-webpack-plugin": "^2.0.0",
    "eslint": "^7.25.0",
    "html-inline-css-webpack-plugin": "1.11.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.1",
    "mini-css-extract-plugin": "^1.6.0",
    "mobx": "^6.3.0",
    "node-sass": "^5.0.0",
    "postcss": "^8",
    "postcss-loader": "^5",
    "postcss-preset-env": "^6.7.0",
    "sass-loader": "^11.0.1",
    "sitemap-webpack-plugin": "^1.1.0",
    "svgo-loader": "^3.0.0",
    "ts-loader": "^9.1.2",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4",
    "underscore": "^1.13.1",
    "underscore-template-loader": "git+https://github.com/idudinov/underscore-template-loader.git#webpack-5",
    "webpack": "^5.36.2",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  }
}
