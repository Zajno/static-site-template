{
  "name": "static-website-template",
  "version": "1.0.1",
  "description": "Static website template by Zajno",
  "private": true,
  "engines": {
    "yarn": "^1.22",
    "node": "^12"
  },
  "scripts": {
    "start": "cd server && yarn && yarn start:ts",
    "emulate": "firebase emulators:start --only hosting",
    "dev": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack-dev-server --host 0.0.0.0 --mode development --color --env.noclear --env.hmr",
    "build": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --display-error-details --mode development",
    "build:ts:w": "tsc --build --watch",
    "build:minify": "yarn build --env.fullminify",
    "build:prod": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --display-error-details --mode production",
    "build:prod:minify": "yarn build:prod --env.fullminify",
    "build:prod:minify:env": "cross-env NODE_ENV=production yarn build:prod:minify",
    "heroku-postbuild": "yarn build:prod:minify && cd server && yarn && yarn build",
    "build:release:staging": "yarn && (cross-env HOSTNAME=https://zajno.com/ yarn build:prod:minify:env)",
    "build:release:production": "yarn && (cross-env HOSTNAME=https://zajno.com/ yarn build:prod:minify:env)",
    "deploy:firebase:staging": "firebase deploy --only hosting:web-stage",
    "deploy:firebase:production": "firebase deploy --only hosting:web-prod",
    "lint": "eslint \"app/**/*.ts\""
  },
  "dependencies": {
    "core-js": "^3.8.3",
    "detect-browser": "^5.2.0",
    "gsap": "file:./gsap-member/npm-install-this/gsap-bonus.tgz",
    "lottie-web": "^5.7.6",
    "mobx": "5",
    "regenerator-runtime": "^0.13.7",
    "reset-css": "^5.0.1",
    "zajno-common-utils": "git+https://github.com/Zajno/common-utils.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/core-js": "^2.5.4",
    "@types/jest": "^26.0.20",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.6.4",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/ts-nameof": "^4.2.1",
    "@types/webpack": "4.41.8",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.3",
    "css-loader": "^4.3.0",
    "eslint": "^7.20.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsdoc": "^31.6.1",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.11.1",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^7",
    "postcss-loader": "^4",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^4",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^10.0.2",
    "sitemap-webpack-plugin": "^1.1.0",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.17",
    "ts-nameof": "^5.0.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.1.5",
    "underscore-template-loader": "^1.1.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "resolutions": {
    "@types/webpack": "4.41.8",
    "mobx": "5.15.7"
  }
}
