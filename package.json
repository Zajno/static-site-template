{
  "name": "static-website-template",
  "version": "0.0.1",
  "description": "Static website",
  "private": true,
  "engines": {
    "yarn": "^1.12",
    "node": "^8.9 || ^10.7"
  },
  "scripts": {
    "start": "node server/index.js",
    "dev": "webpack-dev-server --host 0.0.0.0 --mode development --color --env.noclear",
    "build": "webpack --display-error-details --mode development",
    "build:minify": "npm run build -- --env.fullminify",
    "build:prod": "webpack --display-error-details --mode production",
    "build:prod:minify": "npm run build:prod -- --env.fullminify",
    "build:prod:minify:env": "cross-env NODE_ENV=production npm run build:prod:minify",
    "postinstall": "npm run build:prod:minify && cd server && yarn"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "bodymovin": "^4.13.0",
    "detect-browser": "^3.0.1",
    "gsap": "^2.0.2",
    "jquery": "^3.3.1",
    "reset-css": "^4.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-syntax-decorators": "^7.1.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/preset-react": "^7.0.0",
    "@types/gsap": "^1.20.0",
    "autoprefixer": "^9.4.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^1.0.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.5.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.4.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^3.0.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "underscore-template-loader": "^1.0.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.26.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  }
}
