{
  "name": "static-website-template",
  "version": "0.0.1",
  "description": "Static website template by Zajno",
  "private": true,
  "engines": {
    "yarn": "^1.22",
    "node": "^12"
  },
  "scripts": {
    "start": "node server/index.js",
    "dev": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack-dev-server --host 0.0.0.0 --mode development --color --env.noclear --env.hmr",
    "build": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --display-error-details --mode development",
    "build:minify": "yarn build --env.fullminify",
    "build:prod": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --display-error-details --mode production",
    "build:prod:minify": "yarn build:prod --env.fullminify",
    "build:prod:minify:env": "cross-env NODE_ENV=production yarn build:prod:minify",
    "heroku-postbuild": "yarn build:prod:minify && cd server && yarn"
  },
  "dependencies": {
    "@babel/polyfill": "^7.11.5",
    "core-js": "^3",
    "detect-browser": "^5.1.1",
    "gsap": "./gsap-member/npm-install-this/gsap-bonus.tgz",
    "lottie-web": "^5.7.3",
    "mobx": "^5.15.6",
    "regenerator-runtime": "^0.13.7",
    "reset-css": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/core-js": "^2.5.4",
    "@types/mini-css-extract-plugin": "^0.9.1",
    "@types/node": "^14.6.4",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/ts-nameof": "^4.2.1",
    "@types/webpack": "4.41.8",
    "@types/webpack-dev-server": "^3.11.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^4.3.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.6",
    "file-loader": "^6.1.0",
    "html-loader": "^1.3.0",
    "html-webpack-plugin": "^4.4.1",
    "image-webpack-loader": "^6.0.0",
    "mini-css-extract-plugin": "^0.11.1",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss-loader": "^4.0.1",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^4",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^10.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.3",
