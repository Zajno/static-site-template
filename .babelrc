{
    "presets":[
        ["@babel/preset-env", {
            "useBuiltIns": "entry",
            "targets": {
                "chrome": "58",
                "ie": "11"
            }
        }],
    ],
    "plugins": [
        "@babel/plugin-syntax-dynamic-import",
        ["@babel/plugin-syntax-decorators", { "decoratorsBeforeExport": false }],
        "@babel/plugin-proposal-class-properties",
    ]
}
